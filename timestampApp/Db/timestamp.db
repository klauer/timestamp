record(stringin, "$(Sys)$(Dev)Val:Time-I"){
	field(DTYP, "Soft Timestamp")
	field(SCAN, "Passive")
	field(INP,  "@%s.%f")
	field(SDIS, "$(Inp) CP")
	field(DISA, "-1")
	field(TSEL, "$(Inp).TIME CA")
	field(FLNK, "$(Sys)$(Dev)-aSub")
}

record(mbbo, "$(Sys)$(Dev)Sw-Sel"){
	field(ZRST, "IDLE")
	field(ONST, "CAPTURE")
	field(TWST, "RESET")
	field(ZRVL, "0")
	field(ONVL, "1")
	field(TWVL, "2")
}

record(aSub, "$(Sys)$(Dev)-aSub"){
	field(SNAM, "buffer")
	field(NOA,  "1")
	field(NOB,  "1")
	field(FTA,  "LONG")
	field(FTB,  "DOUBLE")
	field(INPA, "$(Sys)$(Dev)Sw-Sel NPP NMS")
	field(INPB, "$(Sys)$(Dev)Val:Time-I NPP NMS")
	field(NOVA, "$(N)")
	field(FTVA, "DOUBLE")
	field(NOVB, "1")
	field(FTVB, "LONG")
	field(NOVC, "1")
	field(FTVC, "LONG")
	field(FLNK, "$(Sys)$(Dev)-Fout_")
}

record(fanout, "$(Sys)$(Dev)-Fout_"){
	field(LNK1, "$(Sys)$(Dev)Val:Time-Wfrm")
	field(LNK2, "$(Sys)$(Dev)Val:TimeN-I")
	field(LNK3, "$(Sys)$(Dev)Sts-Sts")
}

record(waveform, "$(Sys)$(Dev)Val:Time-Wfrm"){
	field(INP,  "$(Sys)$(Dev)-aSub.VALA NPP MS")
	field(NORD, "10")
	field(NELM, "$(N)")
	field(FTVL, "DOUBLE")
}

record(longin, "$(Sys)$(Dev)Val:TimeN-I"){
	field(INP,  "$(Sys)$(Dev)-aSub.VALB NPP MS")
}

record(mbbi, "$(Sys)$(Dev)Sts-Sts"){
	field(INP,  "$(Sys)$(Dev)-aSub.VALC NPP MS")
	field(ZRST, "OK")
	field(ONST, "FULL")
	field(ZRVL, "0")
	field(ONVL, "1")
	field(ZRSV, "NO_ALARM")
	field(ONSV, "MAJOR")
}
